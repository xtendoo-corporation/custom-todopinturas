<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_import_contacts_wizard" model="ir.ui.view">
        <field name="name">import.contacts.wizard.form</field>
        <field name="model">import.contacts.wizard</field>
        <field name="arch" type="xml">
            <form string="Importar Contactos">
                <group>
                    <field name="file" filename="file_name"/>
                </group>
                <footer>
                    <button name="action_import_contacts" string="Importar" type="object" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
